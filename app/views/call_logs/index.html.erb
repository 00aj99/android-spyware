<p id="notice"><%= notice %></p>

<h1>Call Logs</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Source</th>
      <th>Destination</th>
      <th>Duration</th>
      <th>Filename</th>
      <th>Smartphone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @call_logs.each do |call_log| %>
      <tr>
        <td><%= call_log.date %></td>
        <td><%= call_log.source %></td>
        <td><%= call_log.destination %></td>
        <td><%= call_log.duration %></td>
        <td><%= call_log.filename %></td>
        <td><%= call_log.smartphone_id %></td>
        <td><%= link_to 'Show', call_log %></td>
        <td><%= link_to 'Edit', edit_call_log_path(call_log) %></td>
        <td><%= link_to 'Destroy', call_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Call Log', new_call_log_path %>

<p id="notice"><%= notice %></p>

<h1>Smartphones</h1>

<table>
  <thead>
    <tr>
      <th>Operating system</th>
      <th>Name</th>
      <th>Pictures</th>
      <th>Screenshots</th>
      <th>Videos</th>
      <th>Recordings</th>
      <th>Is rooted</th>
      <th>Call logs</th>
      <th>Contacts</th>
      <th>Sms messages</th>
      <th>Geo locations</th>
      <th>Is app hidden</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @smartphones.each do |smartphone| %>
      <tr>
        <td><%= smartphone.operating_system %></td>
        <td><%= smartphone.name %></td>
        <td><%= smartphone.pictures %></td>
        <td><%= smartphone.screenshots %></td>
        <td><%= smartphone.videos %></td>
        <td><%= smartphone.recordings %></td>
        <td><%= smartphone.is_rooted %></td>
        <td><%= smartphone.call_logs %></td>
        <td><%= smartphone.contacts %></td>
        <td><%= smartphone.sms_messages %></td>
        <td><%= smartphone.geo_locations %></td>
        <td><%= smartphone.is_app_hidden %></td>
        <td><%= link_to 'Show', smartphone %></td>
        <td><%= link_to 'Edit', edit_smartphone_path(smartphone) %></td>
        <td><%= link_to 'Destroy', smartphone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Smartphone', new_smartphone_path %>
